{
  "name": "vscode-babel-repl",
  "displayName": "vscode-babel-repl",
  "description": "Write next generation Javascript and see the transpiled output.",
  "version": "0.0.1",
  "publisher": "t-sauer",
  "engines": {
    "vscode": "^1.7.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.newRepl"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.newRepl",
        "title": "Open new Babel REPL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-preset-latest": "^6.16.0"
  }
}
